                <para ALIGN="3" LINESP="12"/>
                <ITEXT FONT="Candara Bold" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="3."/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH=" Prepare the remaining cards in separate piles as in the base game."/>
                <para ALIGN="3" LINESP="12"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="117.809106470387" YPOS="307.867141732284" OwnPage="0" ItemID="178237264" PTYPE="4" WIDTH="225.971421088668" HEIGHT="14.5072463768117" FRTYPE="0" CLIPEDIT="0" PWIDTH="1.0000003087487" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L225.971 0 L225.971 14.5072 L0 14.5072 L0 0 Z" copath="M0 0 L225.971 0 L225.971 14.5072 L0 14.5072 L0 0 Z" gXpos="-10.5383510977188" gYpos="-5330.26267723975" gWidth="153.017952755905" gHeight="30.0062992125992" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
