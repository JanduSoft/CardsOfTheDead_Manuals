            <StoryText>
                <DefaultStyle LINESP="12"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Bold Italic" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="Showcase Chainsaw"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH=": Si los Puntos de Golpe llegan a zero o se utiliza una carta de &quot;"/>
                <ITEXT FONT="Candara Italic" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH="ToolKit"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH="&quot;, "/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="el jugador activo recibe las cartas de &quot;Conjunto Épico III&quot;, &quot;"/>
                <ITEXT FONT="Candara Italic" FONTSIZE="8" FCOLOR="White" CH="Chainsaw"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="&quot; y &quot;"/>
                <ITEXT FONT="Candara Italic" FONTSIZE="8" FCOLOR="White" CH="Gas Chainsaw"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="&quot; (si no tiene espacio en la mano, deberá descartar alguna carta en el proceso)."/>
                <para ALIGN="3" LINESP="12"/>
                <para ALIGN="3" LINESP="12"/>
                <para ALIGN="3" LINESP="12"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Bold Italic" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="Zombie Doctor"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH=": "/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="Cuando el "/>
                <ITEXT FONT="Candara Italic" FONTSIZE="8" FCOLOR="White" CH="&quot;Zombie Doctor"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="&quot; sea eliminado, el resto de zombies que se encuentren en el &quot;Área de Juego&quot;, retiran todas las fichas de Golpe que tuvieran encima."/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Bold Italic" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="Zombie Scientist"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH=": "/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="Si recibe Daño a Distancia el &quot;Zombie Scientist&quot; explota. En dicho caso, resuelve el Daño Explosivo."/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Bold Italic" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="Zombie Nurse"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH=": "/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="Cada vez que la carta acumule una Ficha de Turno, cada jugador pierde los Puntos de Inmunidad indicados."/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Bold Italic" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="Project: Goliath"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH=": "/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="Cada vez que la carta acumule tres Fichas de Turno, retira todas las Fichas de Golpe que tuviera la carta de &quot;"/>
                <ITEXT FONT="Candara Italic" FONTSIZE="8" FCOLOR="White" CH="Project: Goliath"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="&quot; encima."/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Bold Italic" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="N.B.Q."/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH=": "/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="Una vez se utilice, deja la carta junto a la carta de Superviviente (para indicar que lo lleva puesto). Mientras el Superviviente tenga equipado el &quot;"/>
                <ITEXT FONT="Candara Italic" FONTSIZE="8" FCOLOR="White" CH="N.B.Q."/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="&quot;, "/>
                <ITEXT FONT="Candara Bold" FONTSIZE="8" FCOLOR="White" CH="NO"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH=" puede perder Puntos de Inmunidad. En caso de que el Superviviente reciba Daño que le haga perder Puntos de Golpe, descarta inmediatamente el &quot;"/>
                <ITEXT FONT="Candara Italic" FONTSIZE="8" FCOLOR="White" CH="N.B.Q."/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="&quot;."/>
                <para ALIGN="3" LINESP="12"/>
                <ITEXT FONT="Times New Roman Regular" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="&quot;Nota: El Superviviente puede llevar equipado el &quot;"/>
                <ITEXT FONT="Times New Roman Italic" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="Kevlar"/>
                <ITEXT FONT="Times New Roman Regular" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="&quot; y el &quot;"/>
                <ITEXT FONT="Times New Roman Italic" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="N.B.Q."/>
                <ITEXT FONT="Times New Roman Regular" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="&quot; simultáneamente. Debido a que el &quot;"/>
                <ITEXT FONT="Times New Roman Italic" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="Kevlar"/>
                <ITEXT FONT="Times New Roman Regular" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="&quot; absorbe los Puntos de Golpe, mientras el jugador tenga la carta de &quot;"/>
                <ITEXT FONT="Times New Roman Italic" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="Kevlar"/>
                <ITEXT FONT="Times New Roman Regular" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="&quot; activa, no deberá descartar la carta de &quot;"/>
                <ITEXT FONT="Times New Roman Italic" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="N.B.Q."/>
                <ITEXT FONT="Times New Roman Regular" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="&quot;."/>
                <para ALIGN="3" LINESP="12"/>
                <para ALIGN="3" LINESP="12"/>
                <para ALIGN="3" LINESP="12"/>
                <para ALIGN="3"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Bold Italic" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="Viral Gun"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH=": Cada vez que un jugador dispare con la &quot;"/>
                <ITEXT FONT="Candara Italic" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH="Viral Gun"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH="&quot; pierde los Puntos de Inmunidad indicados."/>
                <para ALIGN="3" LINESP="12"/>
                <ITEXT FONT="Times New Roman Regular" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="&quot;Nota: Si lleva equipado el traje de &quot;"/>
                <ITEXT FONT="Times New Roman Italic" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="N.B.Q."/>
                <ITEXT FONT="Times New Roman Regular" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="&quot;, el jugador no perderá Puntos de Inmunidad cuando dispare."/>
