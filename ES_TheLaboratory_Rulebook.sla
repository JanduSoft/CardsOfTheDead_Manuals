                <para ALIGN="3" LINESP="12"/>
                <para ALIGN="3" LINESP="12"/>
                <para ALIGN="3" LINESP="12"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Bold Italic" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="Workshop"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="&quot;"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH=": "/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="Si un jugador se descarta de una carta de &quot;"/>
                <ITEXT FONT="Candara Italic" FONTSIZE="8" FCOLOR="White" CH="Toolkit"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH="&quot;, cada jugador puede incrementar en "/>
                <ITEXT FONT="Candara Bold" FONTSIZE="8" FCOLOR="White" CH="1"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" CH=" el Daño de una de sus armas (El tipo de Daño es indiferente) que tenga en su mano, Reserva o equipada en ese momento. Si el arma tiene Usos límitados el efecto se perderá una vez deba descartar la carta."/>
                <para ALIGN="3" LINESP="12"/>
                <ITEXT FONT="Times New Roman Regular" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="&quot;Nota: El &quot;"/>
                <ITEXT FONT="Times New Roman Italic" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="Punch"/>
                <ITEXT FONT="Times New Roman Regular" FONTSIZE="6" FCOLOR="Yellow" FSHADE="100" CH="&quot; no es un arma. No puede beneficiarse del efecto."/>
