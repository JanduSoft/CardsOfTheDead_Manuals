                <DefaultStyle LINESP="12"/>
                <ITEXT FONT="Candara Bold" FONTSIZE="9" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="¿Cómo Ganar?"/>
                <para ALIGN="3" LINESP="12"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH="La humanidad aún no está perdida y tiene una oportunidad. Encuentra los "/>
                <ITEXT FONT="Candara Bold" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH="Patógenos"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH=" necesarios y produce una cura para el virus mientras huyes de las hordas de zombies."/>
                <para ALIGN="3" LINESP="12"/>
                <para ALIGN="3" LINESP="12"/>
                <ITEXT FONT="Candara Bold" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="1."/>
                <ITEXT FONT="Candara Bold" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="None" CH=" Victoria"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH=" &quot;Salvar la Humanidad&quot;: si los Supervivientes logran terminar el tercer Día en posesión de los tres Patógenos."/>
                <para ALIGN="3" LINESP="12"/>
                <ITEXT FONT="Candara Bold" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="2."/>
                <ITEXT FONT="Candara Bold" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="None" CH=" Empate"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH=" &quot;Vive un dia más&quot;: si los Supervivientes logran terminar el tercer Día pero no están en posesión de todos los Patógenos."/>
                <para ALIGN="3" LINESP="12"/>
                <ITEXT FONT="Candara Bold" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="Granate_Background" CH="3."/>
                <ITEXT FONT="Candara Bold" FONTSIZE="8" FCOLOR="White" FSHADE="100" BGCOLOR="None" CH=" Derrota"/>
                <ITEXT FONT="Candara Regular" FONTSIZE="8" FCOLOR="White" FSHADE="100" CH=" &quot;Cerebrooos&quot;: todos los Supervivientes han muerto antes de terminar el tercer Día."/>
